{
  "name": "warp-engine Notificiations",
  "version": "1.2",
  "description":
    "Shows desktop notifications from your personal warp-engine",
  "icons": {"16": "16.png", "48": "48.png", "64": "64.png", "128": "128.png"},
  "permissions": [
    "notifications"
  ],
  "options_page": "options.html",
  "background": { "scripts": ["bower_components/reconnectingWebsocket/reconnecting-websocket.js", 
  "bower_components/warp-client/warp.js", "background.js"] },
  "manifest_version": 2,

  "web_accessible_resources": [
    "64.png"
  ]
}
